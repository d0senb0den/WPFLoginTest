<Window x:Class="WPFLoginTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFLoginTest"
        mc:Ignorable="d"
        Background="#2b2b2b"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel x:Name="Profiles" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
        </StackPanel>
        <Button Click="ManageUserButtonClicked" x:Name="ManageUsersButton" Content="Edit" Width="50" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="10"/>
        <Button Visibility="Hidden" Click="BackButtonClicked" x:Name="BackButton" Content="Back" Width="50" Height="25" Margin="10" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
</Window>
